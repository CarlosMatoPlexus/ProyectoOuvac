<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Informacion_concatenada__c</fullName>
    <externalId>false</externalId>
    <formula>TRIM(
   BLANKVALUE(Email, &apos;&apos;) &amp;
   IF(AND(NOT(ISBLANK(Email)), NOT(ISBLANK(Phone))), &apos; - &apos;, &apos;&apos;) &amp;
   BLANKVALUE(Phone, &apos;&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Informacion concatenada</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
